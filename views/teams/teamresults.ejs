<h1>Team Results</h1>



<p>Team name: <%=results.name%></p>
<p>Country: <%=results.country%></p>
<p>Founded: <%=results.founded%></p>
<p><img src='<%=results.logo%>' alt="logo"></p>
<p>Team ID: <%=results.id%></p>

<form action="/teams/standings" method="GET">
    <input type="hidden" id="teamid" name="teamid" value="<%=results.id%>">
  
    <input type="submit" value="view current team standings" class="btn btn-primary">
</form>

<form action="/profile" method="POST">
    <input type="hidden" name="name" id="name" value="<%=results.name%> ">
   <!-- did have a userid input  -->
    <input type="submit" value="save to profile" class="btn btn-primary">
</form>



